 
<h1>All Boards</h1>
<section>
    {{#each board}}
        <article>
            <header></header>
             <a href="/board/{{this.id}}">
            <h1>{{this.name}}</h1>
            </a>
           <h4>{{this.createdAt}}</h4>
            <h4>{{this.updatedAt}}</h4>
        </article>
    {{/each}}
      <a class="btn btn-primary" href="/addprojects" role="button">Add Projects</a>
</section>