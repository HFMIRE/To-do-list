 
<h1>All Boards</h1>
<section>
    {{#each board}}
    <h1>{{this.title}}</h1>
        <article>
            <header></header>
             <a href="/board/{{this.id}}">
            <h1>{{this.name}}</h1>
            </a>
           <h4>{{this.createdAt}}</h4>
            <h4>{{this.updatedAt}}</h4>
        </article>
    {{/each}}
    
</section>